{
  "name": "email-validator-node",
  "description": "A simple package to validate format and MX for an e-mail address.",
  "version": "1.1.7",
  "license": "MIT",
  "repository": "github:bsonmez/email-validator-node",
  "author": {
    "name": "Burak Sonmez",
    "url": "https://github.com/bsonmez"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "publish": "yarn build && npm publish"
  },
  "keywords": [
    "email",
    "validation",
    "validator",
    "domains",
    "disposable"
  ],
  "dependencies": {
    "node-range": "0.1.x"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "jest": "^26.1.0",
    "prettier": "^1.14.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.4"
  }
}
